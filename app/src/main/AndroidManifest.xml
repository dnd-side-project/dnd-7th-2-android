            android:exported="false" />
        <activity
            android:name=".ui.mypage.profile.view.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".ui.mypage.applicationstatus.view.ApplicationStatusActivity"
            android:exported="false" />
        <activity
            android:name=".ui.home.bookmark.view.BookmarkActivity"
            android:exported="false" />
        <activity
            android:name=".ui.home.alarm.view.AlarmActivity"
            android:exported="false" />
        <activity
            android:name=".ui.splash.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.Teamgoo.Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.onboarding.OnboardingActivity"
            android:exported="false" />
